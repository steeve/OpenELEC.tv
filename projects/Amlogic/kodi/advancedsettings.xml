  <fanartres>720</fanartres>
  <imageres>540</imageres>

  <network>
    <cachemembuffersize>0</cachemembuffersize>
    <curlclienttimeout>60</curlclienttimeout>
    <curllowspeedtime>10</curllowspeedtime>
